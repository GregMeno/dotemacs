(type egg)
(signature "md5 egg")
(timestamp 1294245662.0)
(sxml ((tags "egg") (section 2 "md5" (toc)) (section 2 "Documentation" (p "Computes MD5 checksums") (section 3 "Digest Procedures" (p "Any Scheme-object can be the " (tt "SOURCE") " for a digest. See " (int-link "message-digest") " for more information.") (section 4 "md5-primitive" (def (sig (procedure "(md5-primitive)" (id md5-primitive))) (p "Returns the 128-bit checksum primitive object."))) (section 4 "md5-digest ; DEPRECATED" (def (sig (procedure "(md5-digest SOURCE)" (id md5-digest))) (p "Returns the 128-bit checksum of " (tt "SOURCE") " as a hex string."))) (section 4 "md5-binary-digest ; DEPRECATED" (def (sig (procedure "(md5-binary-digest SOURCE)" (id md5-binary-digest))) (p "Returns the 128-bit checksum of " (tt "SOURCE") " as a string."))))) (section 2 "Usage" (highlight scheme "(require-extension md5)")) (section 2 "Requirements" (p (int-link "message-digest"))) (section 2 "Notes" (ul (li "Use the " (tt "message-digest") " " (i "algorithm") " API instead of " (tt "md5-digest") " & " (tt "md5-binary-digest") "."))) (section 2 "Author" (p "Colin Plumb, wrapped for Chicken by " (int-link "/users/peter-bex" "Peter Bex"))) (section 2 "Version history" (dl (dt "2.5") (dd "Make function declarations " (tt "static") " in order to avoid conflicts with those from libc if the OS supplies them. This makes MD5 work on OpenBSD (thanks to Alan Post and Christian Kellermann)") (dt "2.4") (dd "Change deprecated " (tt "pointer") " foreign type specifier to " (tt "scheme-pointer") " to make it work under chickens newer than 4.6.0.") (dt "2.3") (dd "Do not define uint32 as uint32_t but use it directly to avoid conflicts with system-defined types on some OSes.") (dt "2.2") (dd "a " (tt "message-digest-primitive") " has no \"state\". " (int-link "/users/kon-lovett" "Kon Lovett")) (dt "2.1") (dd "Fixed typo in setup file that caused the extension to get installed under the name \"m5\"") (dt "2.0") (dd "Chicken 4 implementation. Replaced GPL code with common public domain code.") (dt "1.1") (dd "Added dependency on message-digest egg") (dt "1.0") (dd "Initial release"))) (section 2 "License" (pre "This code implements the MD5 message-digest algorithm.\nThe algorithm is due to Ron Rivest.  This code was\nwritten by Colin Plumb in 1993, no copyright is claimed.\nThis code is in the public domain; do with it what you wish.") (pre "Equivalent code is available from RSA Data Security, Inc.\nThis code has been tested against that, and is equivalent,\nexcept that you don't need to include two pages of legalese\nwith every copy."))))
